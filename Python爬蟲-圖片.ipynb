    "import requests\n",
    "pic=requests.get('https://imgur.dcard.tw/p3VZwvR.jpg') #圖片網址\n",
    "img2 = pic.content #圖片裡的內容\n",
    "pic_out = open('img1.png','wb') #img1.png為預存檔的圖片名稱\n",
    "pic_out.write(img2) #將get圖片存入img1.png\n",
    "pic_out.close()"
   ]
